{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Nombre de instancia de Data Factory",
			"defaultValue": "adftosnowflake-lab-factory"
		},
		"snowflake_link_service_password": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"password\"de \"snowflake_link_service\""
		},
		"Blobadftosnowflake_sasUri": {
			"type": "secureString",
			"metadata": "Cadena protegida para \"sasUri\"de \"Blobadftosnowflake\""
		},
		"snowflake_link_service_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "adfdemo"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI de la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas."
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Token de SAS para la cuenta de almacenamiento que contiene las plantillas de ARM vinculadas"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adftosnowflake-lab-factory_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"snowflake_link_service_password": {
						"value": "[parameters('snowflake_link_service_password')]"
					},
					"Blobadftosnowflake_sasUri": {
						"value": "[parameters('Blobadftosnowflake_sasUri')]"
					},
					"snowflake_link_service_properties_typeProperties_database": {
						"value": "[parameters('snowflake_link_service_properties_typeProperties_database')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}